{
  "name": "trogers1884/dbdepends",
  "description": "A Laravel package that creates a view for PostgreSQL database dependencies",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Rogers",
      "email": "trogers1884@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0",
    "illuminate/database": "^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Trogers1884\\DBDepends\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trogers1884\\DBDepends\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Trogers1884\\DBDepends\\DBDependsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}